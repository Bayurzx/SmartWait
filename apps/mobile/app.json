{
  "expo": {
    "name": "SmartWait",
    "slug": "smartwait-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartwait.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.smartwait.mobile"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "apiUrl": "http://localhost:3001",
      "wsUrl": "ws://localhost:3001"
    },

    "scheme": "smartwait"
  }
}